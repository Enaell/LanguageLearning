{
  "name": "word",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "spelling": {
      "type": "string",
      "required": true
    },
    "internationalSpelling": {
      "type": "string",
      "required": true
    },
    "language": {
      "type": "string",
      "required": true
    },
    "comment": {
      "type": "string"
    },
    "validated": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "visibility": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "customer",
      "foreignKey": ""
    },
    "translations": {
      "type": "hasMany",
      "model": "translation",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
